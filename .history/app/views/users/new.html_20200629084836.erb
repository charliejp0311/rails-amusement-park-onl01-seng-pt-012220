<%= form_for :user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :height %>
    <%= f.text_field :height %>
    <div>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
    </div>
    <div>
    <%= f.label :nausea %>
    <%= f.text_field :nausea %>
    <%= f.label :happiness %>
    <%= f.text_field :happiness %>
    </div>
    <%= f.submit %>
<% end %>