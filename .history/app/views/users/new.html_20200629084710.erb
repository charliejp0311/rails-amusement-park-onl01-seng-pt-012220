<%= form_for :user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <div>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
    </div>
    <%= f.label :height %>
    <%= f.text_field :height %>

    <%= f.submit %>
<% end %>